{
  "name": "ship/authentication",
  "type": "ship-container",
  "description": "Authentication Container for a nucleus.",
  "require": {
    "php": "^8.3.2",
    "php-64bit": "^8.3.2",
    "laravel/passport": "^12.0.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Containers\\AuthSection\\Authentication\\": "/"
    }
  },
  "extra": {
    "ship": {
      "container": {
        "name": "Authentication"
      }
    },
    "laravel": {
      "providers": [
        "Containers\\AuthSection\\Authentication\\Providers\\MainServiceProvider"
      ]
    }
  }
}
