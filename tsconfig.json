{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noUnusedParameters": false,
    "types": [
      "webpack-env",
      "node"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./app/Containers/AppSection/Greeting/UI/WEB/Asset/@types"
    ],
    "resolveJsonModule": true,
    "paths": {
      "@/*": [
        "app/Containers/AppSection/Greeting/UI/WEB/Asset/*",
        "app/Containers/Auth/UI/WEB/Asset/*",
      ]
    }
  },
  "vueCompilerOptions": {
    "target": 2.7
  },
  "exclude": ["node_modules"],
  "include": [
    "app/Containers/AppSection/Greeting/UI/WEB/Asset",
    "app/Containers/GreetingSection/Auth/UI/WEB/Asset"
  ]
}
