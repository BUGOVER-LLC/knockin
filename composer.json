{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "version": "0.0.2",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-fileinfo": "*",
    "ext-igbinary": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-redis": "*",
    "ext-sockets": "*",
    "ext-swoole": "*",
    "nucleus/repository": "^1.0.0",
    "nucleus/socket": "^0.2.0",
    "guzzlehttp/guzzle": "^7.0.0",
    "laravel/framework": "^10.0.0",
    "laravel/octane": "^1.0.0",
    "laravel/tinker": "^2.0.0",
    "pda/pheanstalk": "^4.0.0",
    "wikimedia/composer-merge-plugin": "^2.0.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.0.0",
    "barryvdh/laravel-ide-helper": "^2.0.0",
    "beyondcode/laravel-er-diagram-generator": "^2.0.0",
    "beyondcode/laravel-query-detector": "^1.0.0",
    "fakerphp/faker": "^1.20.0",
    "friendsofphp/php-cs-fixer": "^3.0.0",
    "laravel/horizon": "^5.0.0",
    "laravel/pint": "^1.0.0",
    "laravel/sail": "^1.0.0",
    "laravel/telescope": "^4.0.0",
    "mockery/mockery": "^1.5.0",
    "nunomaduro/collision": "^7.0.0",
    "orangehill/iseed": "^3.0.1",
    "phpmd/phpmd": "^2.0.0",
    "phpstan/phpstan": "^1.0.0",
    "phpunit/phpunit": "^10.0.0",
    "predis/predis": "^2.0.0",
    "roave/security-advisories": "dev-latest",
    "spatie/laravel-ignition": "^2.0.0",
    "squizlabs/php_codesniffer": "^3.0.0",
    "swoole/ide-helper": "^5.0.0",
    "vimeo/psalm": "^5.0.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "name": "nucleus/repository",
      "url": "https://github.com/BUGOVER-LLC/repository.git",
      "branch": "main"
    },
    {
      "type": "vcs",
      "name": "nucleus/socket",
      "url": "https://github.com/BUGOVER-LLC/socket.git",
      "branch": "main"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Src\\": "src/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "src/_helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('etc/devs/.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "merge-plugin": {
      "include": [
        "app/Ship/composer.json",
        "app/NoixContainers/*/composer.json"
      ],
      "require": [
        "src/service/*/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "ignore-duplicates": false,
      "merge-dev": true,
      "merge-extra": false,
      "merge-extra-deep": false,
      "merge-replace": true,
      "merge-scripts": false
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "wikimedia/composer-merge-plugin": true
    },
    "github-oauth": {
      "github.com": "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"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
