{
  "name": "sea/to-sea",
  "type": "project",
  "description": "sea to sea project.",
  "version": "0.1.0",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "<=8.2.3",
    "php-64bit": "<=8.2.3",
    "ext-gd": "*",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-zlib": "*",
    "ext-redis": "*",
    "ext-pcntl": "*",
    "ext-swoole": "*",
    "ext-bcmath": "*",
    "ext-imagick": "*",
    "ext-openssl": "*",
    "ext-sockets": "*",
    "ext-igbinary": "*",
    "ext-mbstring": "*",
    "ext-calendar": "*",
    "guzzlehttp/guzzle": "^7.0.0",
    "laravel/framework": "^10.0.0",
    "laravel/octane": "^2.0.0",
    "laravel/tinker": "^2.0.0",
    "pda/pheanstalk": "^5.0.0",
    "shipyard/manager": "^1.0.0",
    "wikimedia/composer-merge-plugin": "^2.0.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.0.0",
    "barryvdh/laravel-ide-helper": "^2.0.0",
    "beyondcode/laravel-er-diagram-generator": "^2.0.0",
    "beyondcode/laravel-query-detector": "^1.0.0",
    "fakerphp/faker": "^1.0.0",
    "friendsofphp/php-cs-fixer": "^3.0.0",
    "jetbrains/phpstorm-stubs": "^2023.2",
    "laravel/homestead": "^14.5.0",
    "laravel/horizon": "^5.0.0",
    "laravel/pint": "^1.0.0",
    "laravel/sail": "^1.0.0",
    "laravel/telescope": "^4.0.0",
    "mockery/mockery": "^1.5.0",
    "nunomaduro/collision": "^7.0.0",
    "orangehill/iseed": "^3.0.1",
    "phpmd/phpmd": "^2.0.0",
    "phpstan/phpstan": "^1.0.0",
    "phpunit/phpunit": "^10.0.0",
    "predis/predis": "^2.0.0",
    "roave/security-advisories": "dev-latest",
    "spatie/laravel-ignition": "^2.0.0",
    "squizlabs/php_codesniffer": "^3.0.0",
    "swoole/ide-helper": "^5.0.0",
    "vimeo/psalm": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('etc/devs/.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "hook-test": "sh bin/package_links"
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "merge-plugin": {
      "include": [
        "app/Ship/composer.json",
        "app/Containers/*/composer.json",
        "app/Containers/*/*/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "merge-dev": true,
      "merge-extra": false,
      "merge-extra-deep": false,
      "merge-scripts": true
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "wikimedia/composer-merge-plugin": true,
      "composer/installers": true
    },
    "platform": {
      "PHP": "8.2"
    }
  },
  "provide": {
    "ext-gd": "*",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-zlib": "*",
    "ext-redis": "*",
    "ext-pcntl": "*",
    "ext-swoole": "*",
    "ext-bcmath": "*",
    "ext-imagick": "*",
    "ext-openssl": "*",
    "ext-sockets": "*",
    "ext-igbinary": "*",
    "ext-mbstring": "*",
    "ext-calendar": "*"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
